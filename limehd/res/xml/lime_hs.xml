<!-- 	xmlns:limehd="http://schemas.android.com/apk/res/net.toload.main.hd" -->
<Keyboard xmlns:limehd="http://schemas.android.com/apk/res/net.toload.main.hd"
	limehd:keyWidth="10%p" limehd:horizontalGap="0px"
	limehd:verticalGap="0px" limehd:keyHeight="@dimen/key_height">

	<Row>
		<Key limehd:codes="49" limehd:keyLabel="1" limehd:keyEdgeFlags="left" />
		<Key limehd:codes="50" limehd:keyLabel="2" /> 
<!-- 		limehd:testAttribute="testing ..." /> -->
		<Key limehd:codes="51" limehd:keyLabel="3" />
		<Key limehd:codes="52" limehd:keyLabel="4" />
		<Key limehd:codes="53" limehd:keyLabel="5" />
		<Key limehd:codes="54" limehd:keyLabel="6" />
		<Key limehd:codes="55" limehd:keyLabel="7" />
		<Key limehd:codes="56" limehd:keyLabel="8" />
		<Key limehd:codes="57" limehd:keyLabel="9" />
		<Key limehd:codes="48" limehd:keyLabel="0" limehd:keyEdgeFlags="right" />
	</Row>

	<Row>
		<Key limehd:codes="81" limehd:keyLabel="Q" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="87" limehd:keyLabel="W"/>
        <Key limehd:codes="69" limehd:keyLabel="E"/>
        <Key limehd:codes="82" limehd:keyLabel="R"/>
        <Key limehd:codes="84" limehd:keyLabel="T"/>
        <Key limehd:codes="89" limehd:keyLabel="Y"/>
        <Key limehd:codes="85" limehd:keyLabel="U"/>
        <Key limehd:codes="73" limehd:keyLabel="I"/>
        <Key limehd:codes="79" limehd:keyLabel="O"/>
        <Key limehd:codes="80" limehd:keyLabel="P" limehd:keyEdgeFlags="right"/>
	</Row>

	<Row>
		<Key limehd:codes="65" limehd:keyLabel="A"  limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="83" limehd:keyLabel="S"/>
        <Key limehd:codes="68" limehd:keyLabel="D"/>
        <Key limehd:codes="70" limehd:keyLabel="F"/>
        <Key limehd:codes="71" limehd:keyLabel="G"/>
        <Key limehd:codes="72" limehd:keyLabel="H"/>
        <Key limehd:codes="74" limehd:keyLabel="J"/>
        <Key limehd:codes="75" limehd:keyLabel="K"/>
        <Key limehd:codes="76" limehd:keyLabel="L"/>
		<Key limehd:codes="59" limehd:keyLabel=";" limehd:keyEdgeFlags="right" />
	</Row>

	<Row>
		<Key limehd:codes="90" limehd:keyLabel="Z" limehd:keyEdgeFlags="left" />
        <Key limehd:codes="88" limehd:keyLabel="X"/>
        <Key limehd:codes="67" limehd:keyLabel="C"/>
        <Key limehd:codes="86" limehd:keyLabel="V"/>
        <Key limehd:codes="66" limehd:keyLabel="B"/>
        <Key limehd:codes="78" limehd:keyLabel="N"/>
        <Key limehd:codes="77" limehd:keyLabel="M"/>
		<Key limehd:codes="91" limehd:keyLabel="[" />
		<Key limehd:codes="93" limehd:keyLabel="]" />
		<Key limehd:codes="-5" limehd:keyIcon="@drawable/sym_keyboard_delete" limehd:iconPreview="@drawable/sym_keyboard_feedback_delete" limehd:isModifier="true"
			limehd:isRepeatable="true" />
	</Row>


	<Row limehd:rowEdgeFlags="bottom">
		<Key limehd:codes="-3"  limehd:keyIcon="@drawable/sym_keyboard_done" limehd:iconPreview="@drawable/sym_keyboard_feedback_done" limehd:isModifier="true"
			limehd:keyEdgeFlags="left" />
		<Key limehd:codes="-9" 
				limehd:keyLabel="EN"  limehd:isModifier="true" 
				limehd:keyWidth="10%p" 
				/>
		<Key limehd:codes="45" limehd:keyLabel="-" />
		<Key limehd:codes="61" limehd:keyLabel="=" />
		<Key limehd:codes="46" 
				limehd:keyLabel="."
				 limehd:popupKeyboard="@xml/popup_c_punctuation"   
				limehd:keyWidth="10%p" />
		<Key limehd:codes="32"
			limehd:keyIcon="@drawable/sym_keyboard_space_lime" limehd:iconPreview="@drawable/sym_keyboard_feedback_space"
			limehd:keyWidth="10%p" limehd:isRepeatable="true" />
		<Key limehd:codes="44" limehd:keyLabel="," limehd:keyWidth="10%p" />
        <Key limehd:codes="39" limehd:keyLabel="\'"/>
		<Key limehd:codes="47" limehd:keyLabel="/" limehd:keyWidth="10%p" />
		<Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return" limehd:iconPreview="@drawable/sym_keyboard_feedback_return" limehd:isModifier="true"
			limehd:keyEdgeFlags="right"/>
	</Row>
</Keyboard>